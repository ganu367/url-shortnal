{
  "devCommand": "gunicorn -w 4 -k uvicorn.workers.UvicornWorker main_gunicorn:app",
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9" 
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ]
}
